[Unit]
Description=D2D Convergence Manager Daemon
#Wants=net-config.service,bt-service
#After=net-config.service,bt-service

[Service]
Type=dbus
User=system
Group=system
SmackProcessLabel=d2d-conv-manager
BusName=org.tizen.d2d-conv-manager
ExecStart=/usr/bin/d2d-conv-managerd
Restart=always
RestartSec=1

[Install]
WantedBy=graphical.target